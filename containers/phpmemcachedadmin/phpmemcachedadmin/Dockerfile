ARG DOCKER_REGISTRY=docker.io
ARG DOCKER_CONTAINER_VERSAO_PRODUTO=3.5.1
ARG IMAGEM_BASE_PHPMEMCACHEDADMIN=${DOCKER_REGISTRY}/moonbuggy2000/phpmemcachedadmin
ARG IMAGEM_BASE_PHPMEMCACHEDADMIN_VERSAO=1.3.0-ppc64le
    

FROM ${IMAGEM_BASE_PHPMEMCACHEDADMIN}:${IMAGEM_BASE_PHPMEMCACHEDADMIN_VERSAO}

ADD Memcache.php /usr/share/nginx/html/Config/
ADD ./start.sh /start.sh